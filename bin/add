#!/usr/bin/python3


#Description: add the parameter to the conf repo

import os, subprocess

_bin_path = os.path.expanduser("~/conf")

def main(files):
	
	for filename in files:
		path = os.path.realpath(filename)
		homedir = os.path.expanduser("~")

		if homedir in path:
			subprocess.check_output(["{}/add_conf_file".format(_bin_path), path], cwd = homedir)
		else:
			print("not done :{}, reason {}", filename, "not in home directory")


if __name__ == "__main__":
	main(file[1:])

